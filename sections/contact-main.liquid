{{ 'section-contact-main.css' | asset_url | stylesheet_tag }}
<section id="contacts-page">
    <div class="container contact-height">
        <div class="row d-flex justify-content-center">
            <div class="col-lg-12">
                <h2 class="contact-title text-center">
                    Do you have questions?
                </h2>
                <p class="contact-description text-center">
                    Contact us at <a href="mailto:{{ section.settings.contact-email }}" class="contact-link" target="_blank">{{ section.settings.contact-email }}</a>
                </p>
            </div>
        </div>
    </div>
</section>
{% schema %}
{
"name": "t:sections.contact-form.name",
"tag": "section",
"class": "section",
"settings": [
{
"type": "text",
"id": "contact-email",
"default": "care@wellinda.health",
"label": "Contact us email"
}]
}
{% endschema %}
